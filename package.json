{
  "name": "filename-lint",
  "displayName": "File Name Lint",
  "description": "A vscode extension that enforces consistent naming conventions for your files and folders.",
  "author": "abgox",
  "publisher": "abgox",
  "version": "0.1.0",
  "license": "MIT",
  "categories": [
    "Linters"
  ],
  "keywords": [
    "lint",
    "linter",
    "validator",
    "rules",
    "checker",
    "validator",
    "conventions",
    "name",
    "filename",
    "file name",
    "naming rules",
    "naming conventions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abgox/filename-lint"
  },
  "homepage": "https://github.com/abgox/filename-lint",
  "icon": "icon.png",
  "bugs": {
    "url": "https://github.com/abgox/filename-lint/issues"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filename-lint.check-manually",
        "title": "%filename-lint.check-manually%"
      }
    ],
    "configuration": {
      "title": "filename-lint",
      "properties": {
        "filename-lint.enabled": {
          "description": "%filename-lint.enabled%",
          "enum": [
            true,
            false
          ],
          "default": false,
          "scope": "resource"
        },
        "filename-lint.namingPattern": {
          "type": "string",
          "markdownDescription": "%filename-lint.namingPattern%",
          "description": "%filename-lint.namingPattern%",
          "examples": [
            "lowercase",
            "kebab-case",
            "snake_case",
            "camelCase",
            "PascalCase"
          ],
          "minLength": 1,
          "default": "lowercase",
          "scope": "resource"
        },
        "filename-lint.excludePatterns": {
          "type": "array",
          "markdownDescription": "%filename-lint.excludePatterns%",
          "description": "%filename-lint.excludePatterns%",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "default": [
            "**/.git/**",
            "**/.idea/**",
            "**/node_modules/**"
          ],
          "minItems": 1,
          "scope": "resource"
        },
        "filename-lint.includePatterns": {
          "type": "array",
          "markdownDescription": "%filename-lint.includePatterns%",
          "description": "%filename-lint.includePatterns%",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "default": [
            "**/**"
          ],
          "minItems": 1,
          "scope": "resource"
        },
        "filename-lint.presetPattern": {
          "deprecationMessage": "%filename-lint.deprecated.presetPattern%"
        },
        "filename-lint.customRegex": {
          "deprecationMessage": "%filename-lint.deprecated.customRegex%"
        },
        "filename-lint.ignorePatterns": {
          "deprecationMessage": "%filename-lint.deprecated.ignorePatterns%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.3.2",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "minimatch": "^10.0.1",
    "vscode-nls-i18n": "^0.2.4"
  }
}
